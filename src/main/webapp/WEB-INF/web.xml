<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">


	<error-page>
		<error-code>404</error-code>
		<location>/error_404.jsp</location>
	</error-page>


	<display-name>LoginJSP</display-name>
	<welcome-file-list>
		<welcome-file>signup.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServ</servlet-name>
		<servlet-class>com.start.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServ</servlet-name>
		<url-pattern>/Log</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GenServ</servlet-name>
		<servlet-class>com.start.Second</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GenServ</servlet-name>
		<url-pattern>/Gen</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegisterServ</servlet-name>
		<servlet-class>com.start.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServ</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SuccessServ</servlet-name>
		<servlet-class>com.start.SucessServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SuccessServ</servlet-name>
		<url-pattern>/success</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Serv</servlet-name>
		<servlet-class>com.attr.Servlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Serv</servlet-name>
		<url-pattern>/Servlet2</url-pattern>
	</servlet-mapping>




</web-app>